 <record model="ir.actions.act_window" id="action_sales_visit">
    <field name="name">Sales Visits List</field>
    <field name="res_model">sale.visit</field>
    <field name="view_mode">tree,form,pivot,calendar</field>
    <field name="target">inline</field>

   
</record>



<record model="ir.actions.act_window" id="action_sales_visit_planned">
    <field name="name">Sales Visits Planned</field>
    <field name="res_model">sale.visit.planned</field>
    <field name="view_mode">tree,form</field>
    <field name="target">inline</field>

   
</record>
<record model="ir.actions.act_window" id="action_sales_visit_activity">
    <field name="name">Sales Visits activity</field>
    <field name="res_model">sale.visit</field>
    <field name="view_mode">tree,form</field>
    <field name="domain">[('state','=','Approve')]</field>
    <field name="target">inline</field>
    
   
</record>

